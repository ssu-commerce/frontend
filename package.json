{
    "name": "ssu-commerce-fe",
    "private": true,
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "clean": "turbo run clean && rm -rf node_modules",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "changeset": "changeset",
        "version-packages": "changeset version",
        "release": "turbo run build --filter=sb^... && changeset publish"
    },
    "devDependencies": {
        "@changesets/cli": "^2.25.2",
        "eslint": "^8.48.0",
        "prettier": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.3",
        "tsconfig": "workspace:*",
        "turbo": "latest"
    },
    "packageManager": "pnpm@8.7.4"
}
