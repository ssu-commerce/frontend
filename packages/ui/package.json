{
  "name": "@sc/ui",
  "version": "0.1.0",
  "license": "MIT",
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./style.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/ --fix --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@sc/utils": "workspace:*",
    "@sc/shared": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "eslint-config-sc": "workspace:*",
    "@sc-config/tailwind": "workspace:*",
    "tsconfig-sc": "workspace:*",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.5",
    "postcss": "^8.4.20",
    "autoprefixer": "^10.4.13",
    "tsup": "^6.7.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
